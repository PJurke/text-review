{
  "name": "text-review",
  "version": "0.1.0",
  "private": true,
  "scripts": {

    "dev": "next dev",


    "db:reset": "npx prisma db push --force-reset && npm run db:seed",
    "db:migrate:dev": "npx prisma migrate dev",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:seed": "node ./seeds/seed-database.js",

    "migrate:create": "npx migrate-mongo create",
    "migrate:dev": "dotenv -e .env.local -- npx migrate-mongo up",
    "migrate:prod": "dotenv -e .env.production -- npx migrate-mongo up",
    "migrate:dev:down": "dotenv -e .env.local -- npx migrate-mongo down",
    "migrate:prod:down": "dotenv -e .env.production -- npx migrate-mongo down",

    "build": "npx prisma generate && next build",
    "start": "next start",

    "lint": "next lint",

    "test": "jest",
    "test:watch": "jest --watch"

  },
  "dependencies": {
    "@apollo/client": "^3.12.3",
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.11.1",
    "dotenv": "^17.2.1",
    "graphql": "^16.9.0",
    "next": "^15.4.4",
    "next-auth": "^5.0.0-beta.28",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-loading-skeleton": "^3.5.0",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "winston-transport": "^4.9.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/client-preset": "^4.8.3",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.1.0",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "dotenv-cli": "^9.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.14",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "migrate-mongo": "^12.1.3",
    "postcss": "^8.5.3",
    "prisma": "^6.11.1",
    "tailwindcss": "^4.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
